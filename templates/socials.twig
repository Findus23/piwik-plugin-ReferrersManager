<h3>List of all socials known to piwik</h3>

<p></p>

<table class="dataTable entityTable">

    <thead>
    <tr>
        <th>Name</th>
        <th>Url</th>
        <th>Actions</th>
    </tr>
    </thead>
    <tbody>
    {% for name,infos in socialInfos %}
        {% for i,url in infos %}
            <tr>
                {% if i == 0 %}
                    <td rowspan="{{ infos|length }}">
                        <img src="{{ attribute(socialLogos, name) }}">
                        {{ name }}
                    </td>
                {% endif %}
                <td>{{ url }}</td>
                {% if i == 0 %}
                    <td rowspan="{{ infos|length }}">
                <span class="link_but">
                    <img src="plugins/Morpheus/images/ico_delete.png" title="{{ 'General_Delete'|translate }}"
                         border="0">
                    <span>{{ 'General_Delete'|translate }}</span>
                </span>
                        <br/>
                <span class="link_but">
                    <img src="plugins/Morpheus/images/ico_edit.png" title="{{ 'General_Edit'|translate }}" border="0">
                    <span>{{ 'General_Edit'|translate }}</span>
                </span>
                    </td>
                {% endif %}
            </tr>
        {% endfor %}
    {% endfor %}
    </tbody>
</table>